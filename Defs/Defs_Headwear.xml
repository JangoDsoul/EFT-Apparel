<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--==============================================
	================== Balaclave =====================
	===============================================-->	

  <ThingDef ParentName="JDSFrontierFaceMaskBase">
	<defName>JDSTarkov_Headwear_Balaclava</defName>
	<label>Balaclava</label>
	<description>"A balaclava, also known as a balaclava helmet or Bally or ski mask, is a form of cloth headgear designed to expose only part of the face, usually the eyes and mouth."</description>
	<graphicData>
      <texPath>Things/FaceMask/Balaclava/Balaclava_UI</texPath>
    </graphicData>
    <apparel>
	  <wornGraphicPath>Things/FaceMask/Balaclava/Balaclava</wornGraphicPath>
	  <tags>
		<li>...</li>			
	  </tags>
    </apparel>	
    <statBases>
	  <MaxHitPoints>150</MaxHitPoints>
	  <Mass>2</Mass>
	  <ArmorRating_Sharp>0.7</ArmorRating_Sharp>
	  <ArmorRating_Blunt>0.62</ArmorRating_Blunt>
	  <ArmorRating_Heat>0.5</ArmorRating_Heat>
	  <Insulation_Cold>5</Insulation_Cold>
	  <Insulation_Heat>8</Insulation_Heat>
	  <EquipDelay>2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
	</equippedStatOffsets>	
  </ThingDef>
  
